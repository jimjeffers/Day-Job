  <title>Projects: <%= controller.action_name %></title>
  <link rel="Shortcut Icon" type="images/ico" href="/favicon.ico" />
  
  <%= render :partial => 'layouts/head' %>
</head>
<body>
  <%= render :partial => 'layouts/alerts' %>
  <div id="content" class="content">
